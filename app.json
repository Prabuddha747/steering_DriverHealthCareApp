{
  "expo": {
    "name": "DriverHealthApp",
    "slug": "DriverHealthApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/frontpage.png",
    "scheme": "driverhealthapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.driverhealthapp.app",
      "adaptiveIcon": {
        "backgroundColor": "#1e1b2e",
        "foregroundImage": "./assets/images/frontpage.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/frontpage.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/frontpage.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1e1b2e",
          "dark": {
            "backgroundColor": "#1a0a2e"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2d30ca79-2372-4079-88b0-70901ce4faa4"
      }
    },
    "owner": "prabuddha_verma"
  }
}
